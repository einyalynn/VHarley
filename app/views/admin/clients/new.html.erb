<div class="row-fluid">
  <div class="span12">
    <div class="featured-area">
<p><%= link_to "Back to List", admin_clients_path %>  </p>

      <%= form_for [:admin, @client] do |f| %>


          <%= label_tag(:name, "Client name:") %>
          <%= f.text_field :name, 'required' => '' %>
          <br />

          <%= label_tag(:url, "Client url:") %>
          <%= f.text_field :url %>
          <br />

          <%= label_tag(:sequence, "Sequence:") %>
          <%= f.number_field :sequence %>
          <br />
          <%= submit_tag("Save") %>
      <% end %>
      </div>
    </div>
</div>